package hansome_8;

public class simple_linear_regression {

	public simple_linear_regression(int x[], int y[]) {
		data_set date = new data_set();
		
		System.out.print("en esta parte sacare la media");
		double mediax=0;
		double mediay=0;
		for(int i=0;i<=x.length;i++) {
			mediax=mediax + date.x[i];
			mediay=mediay + date.y[i];
		}//end for
		mediax=mediax/x.length;
		mediay=mediay/y.length;
		System.out.print("media de x: "+mediax);
		System.out.print("media de y: "+mediay);
	}//end simple_linear_regression
}//end class simple_linear_regression
